#include <stdio.h>

int LSearch(int ar[], int len, int target)
{
    for(int i = 0; i< len; i++){
        // 타겟이 있다면 해당 타겟 인덱스를 반환
        if(target == 1)
    }
    // 타겟이 없다면 없음(-1)을 반환
    if()
    {
        
        return ar[len];
    }
    else
    {
         return -1;   
    }
}

int main(void)
{
	int arr[]={3, 5, 2, 4, 9};
	int idx;

	idx=LSearch(arr, sizeof(arr)/sizeof(int), 4);
	if(idx==-1)
		printf("탐색 실패 \n");
	else
		printf("타겟 저장 인덱스: %d \n", idx);

	idx=LSearch(arr, sizeof(arr)/sizeof(int), 7);
	if(idx==-1)
		printf("탐색 실패 \n");
	else
		printf("타겟 저장 인덱스: %d \n", idx);

	return 0;
}
